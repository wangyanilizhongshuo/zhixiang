.portrait-box{
    position: fixed;
    top: 0;
    background-color: rgba(0, 0, 0, 0.43);
    width: 100%;
    height: 100%;
    z-index: 2100;
    cursor: pointer;
    display:none;
}
.portrait-modal{
position: absolute;
bottom: 0;
left: 0;
z-index: 11000;
width: 100%;
-webkit-transform: translate3d(0,100%,0);
transform: translate3d(0,100%,0);
}
.portrait-modal.modal-an {
/* -webkit-transition-duration: .3s; */
-webkit-transition-duration: .3s;
        transition-duration: .3s;
-webkit-transform: translate3d(0,0,0);
transform: translate3d(0,0,0);
}
.portrait-modal-group {
margin: .4rem;
}
.portrait-modal-button,.portrait-modal-button2{
position: relative;
display: inline-block;
vertical-align: top;
width: 100%;
box-sizing: border-box;
margin: 0;
font-weight: 400;
text-align: center;
background: rgba(243,243,243,.95);
border-bottom: 1px solid #d2d2d6;
font-size: 1rem;
height: 2.15rem;
line-height: 2.15rem;
color: #0894ec;
cursor: pointer;
border-radius: .2rem;
}
#felladd ._div:nth-child(2) {
position: relative;
width:100%!important;
height:100%!important;
}
#felladd input {
position: absolute;
font-size: 0;
left: 0;
top: 0;
width: 100%;
height: 100%;
opacity: .01;
filter: Alpha(opacity=1);
cursor: pointer;
}
#felladd .label{
z-index:-11;
}
#datetime-picker{ text-align: right; cursor: pointer;}
.changeN-box {
    position: fixed;
    top: 0;
    background: #efeff4;
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 10000;
    cursor: pointer;
    display: none;
}

